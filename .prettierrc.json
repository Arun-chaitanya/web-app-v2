{
  "singleQuote": true,
  "printWidth": 120,
  "semi": true,
  "importOrder": [
    "^(?!(@|[./])).*",
    "^@(mui|visx|onflow)(.*)$",
    "^@(types)(.*)$",
    "^@(dapp|store|hooks)(.*)$",
    "^@(atoms|molecules|organisms|templates|core|pages)(.*)$",
    "^@(assets|constants)(.*)$",
    "^@(styles)(.*)$",
    "^@(translations)(.*)$",
    "^[./]",
    "^.*$",
    ".scss$"
  ],
  "importOrderSeparation": true
}
